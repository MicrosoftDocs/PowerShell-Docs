{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "resource": [
        {
          "files": [
          "**/images/**",
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.bmp",
          "**/*.html",
          "**/*.css"
          ],
        "exclude": [
          "**/obj/**",
          "_themes/**"
        ]
      }
    ],
    "dest": "InTuneDocs/DeployUse",
    "template": "docs.html",
    "globalMetadata": {
      "layout": "Conceptual",
      "breadcrumb_path": "/enterprise-mobility/toc.json",
      "product_feedback_displaytext" : "Intune Feedback",
      "product_feedback_url": "https://microsoftintune.uservoice.com/"
    }
  }
}
