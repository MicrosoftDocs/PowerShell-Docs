        "globalMetadata": {
            "toc_preview": true,
            "breadcrumb_path": "bread/toc.yml",
            "uhfHeaderId": "MSDocsHeader-Powershell",
            "ROBOTS": "INDEX, FOLLOW",
            "ms.tgt_pltfr": "windows",
