            { "files": ["**/*.yml", "**/About/*.md"], "src": "7.1", "version": "powershell-7.1", "exclude": ["docs-conceptual/**"], "dest": "module" },

            { "files": ["**/*.md"], "src": "virtual-directory", "dest": "scripting" },
            { "files": ["**/*.md"], "src": "virtual-directory-module", "dest": "module" }
