            { "files": ["**/*.md", "**/**.yml"], "src": "docs-conceptual", "version": "powershell-5.1", "dest": "scripting" },
            { "files": ["toc.yml"], "src": "5.1", "version": "powershell-5.1", "dest": "module/psdocs" },
            { "files": ["**/*.yml", "**/About/*.md"], "src": "5.1", "version": "powershell-5.1", "exclude": ["docs-conceptual/**"], "dest": "module" },

