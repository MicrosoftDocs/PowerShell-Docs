{
    "build": {
        "content": [
            { "files": ["**/**.md"], "exclude": ["**/obj/**"] }
        ],
        "globalMetadata": {
            "ROBOTS": "INDEX, FOLLOW",
            "ms.prod": "powershell",
            "ms.technology": "powershell",
            "ms.topic": "conceptual",
            "manager": "carmonm",
            "ms.devlang": "powershell",
            "ms.tgt_pltfr": "windows, macos, linux",
            "author": "sdwheeler",
            "ms.author": "sewhee",
            "feedback_system": "GitHub",
            "feedback_github_repo": "PowerShell/PowerShell-Docs",
